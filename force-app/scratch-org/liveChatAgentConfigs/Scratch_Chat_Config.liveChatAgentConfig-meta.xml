        <profiles>
            <profile>admin</profile>
            <profile>standard</profile>
        </profiles>
    </assignments>
    <customAgentName>{!User_NKS_FullName} ({!User_CRM_NAV_Ident})</customAgentName>
    <disableTransferConferenceGreeting>false</disableTransferConferenceGreeting>
    <enableAgentFileTransfer>false</enableAgentFileTransfer>
    <enableAgentSneakPeek>false</enableAgentSneakPeek>
    <enableAssistanceFlag>false</enableAssistanceFlag>
